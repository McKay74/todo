<h2>Current Lists</h2>
<div class="col-md-4 pull-right">
  <%= link_to "New List", new_list_path, class: 'btn btn-success' %>
</div>
<% @lists.each do |list| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading col-md-4">
        <%= link_to list.name, list %>
      </h4>
      <div class="col-md-4">
        <small>Days left: </small>
      </div>
    </div>    
  </div>
<% end %>


